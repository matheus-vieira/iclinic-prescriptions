  },
  "homepage": "https://github.com/matheus-vieira/iclinic-prescriptions#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cache-manager": "^3.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.0",
    "morgan": "^1.10.0",
